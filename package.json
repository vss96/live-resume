{
  "name": "live-resume",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start:en": "ng serve --configuration=en",
    "start:pt": "ng serve --configuration=pt",
    "build": "ng build --prod",
    "build:en": "ng build --prod --configuration=en",
    "build:pt": "ng build --prod --configuration=pt",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "int:extract": "ng xi18n --output-path src/locales",
    "build-locale:en": "ng build --aot --prod --i18n-locale=en --i18n-format=xlf --i18n-file=src/locales/messages.en.xlf --output-path=dist/en --base-href /en/",
    "build-locale:pt": "ng build --aot --prod --i18n-locale=pt --i18n-format=xlf --i18n-file=src/locales/messages.pt.xlf --output-path=dist/pt --base-href /pt/",
    "build-locale": "npm run build-locale:en && npm run build-locale:pt"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.7",
    "@angular/common": "^9.0.7",
    "@angular/compiler": "^9.0.7",
    "@angular/core": "^9.0.7",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "^9.0.7",
    "@angular/localize": "^9.1.0",
    "@angular/platform-browser": "^9.0.7",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.0.7",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "compass-mixins": "^0.12.10",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "firebase": "^7.24.0",
    "hammerjs": "^2.0.8",
    "http-proxy": "^1.18.1",
    "ini": "^2.0.0",
    "ng-navigator-share": "^1.0.4",
    "node-forge": "^0.10.0",
    "rxjs": "^6.6.7",
    "serialize-javascript": "^4.0.0",
    "socket.io": "^2.4.1",
    "tar": "^6.1.11",
    "tslib": "^1.14.1",
    "webpack-subresource-integrity": "^1.5.2",
    "websocket-extensions": "^0.1.4",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.10",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~5.4.3",
    "ts-node": "~10.4.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
